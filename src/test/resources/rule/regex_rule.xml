<?xml version="1.0" encoding="UTF-8"?>

<rules>
	<rule>
		<name>Attack</name>
		<type>firewall</type>
		<pattern>Firewall Log: .*attack.*</pattern>
		<time>12:20:00 - 20:00:00</time>
		<ip>192.168.0.1</ip>
		<level>1</level>
	</rule>
	<rule>
		<name>Deny</name>
		<type>firewall</type>
		<pattern>Firewall Log: .*(deny|drop).*</pattern>
		<time>*</time>
		<ip>*</ip>
		<level>1</level>
	</rule>
</rules>