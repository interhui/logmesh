<?xml version="1.0" encoding="UTF-8"?>

<rules>
	<rule>
		<name>ASA-Deny</name>
		<type>firewall</type>
		<time>10:00:00 - 23:30:00</time>
		<ip>192.168.0.1</ip>
		<level>1</level>
		<pattern>
			<expression>name &amp;&amp; message</expression>
			<item>
				<key>name</key>
				<value>.*(PIX|ASA).*</value>
			</item>
			<item>
				<key>message</key>
				<value>.*(deny|drop).*</value>
			</item>
		</pattern>
	</rule>
	
	<rule>
		<name>ASA-192.168.0.32</name>
		<type>firewall</type>
		<time>*</time>
		<ip>192.168.0.1</ip>
		<level>1</level>
		<pattern>
			<expression>name &amp;&amp; message</expression>
			<item>
				<key>name</key>
				<value>.*(PIX|ASA).*</value>
			</item>
			<item>
				<key>message</key>
				<value>.*(192.168.0.32).*</value>
			</item>
		</pattern>
	</rule>
	
	<rule>
		<name>ASA-HighLevel</name>
		<type>firewall</type>
		<time>*</time>
		<ip>192.168.0.1</ip>
		<level>1</level>
		<pattern>
			<expression>name &amp;&amp; (level || message)</expression>
			<!--  expression><![CDATA[name && (level || message)]]></expression> -->
			<item>
				<key>name</key>
				<value>.*(PIX|ASA).*</value>
			</item>
			<item>
				<key>level</key>
				<value>3</value>
			</item>
			<item>
				<key>message</key>
				<value>.*(attack).*</value>
			</item>
		</pattern>
	</rule>
</rules>